{
    "id": "microsoft_dnsserver-windows-dc-dns-default",
    "name": "microsoft_dnsserver (windows-dc-dns-default)",
    "namespace": "",
    "policy_id": "windows-dc-dns-default",
    "package": {
      "name": "microsoft_dnsserver",
      "version": ""
    },
    "inputs": [
        {
          "type": "etw",
          "policy_template": "microsoft_dnsserver",
          "enabled": true,
          "streams": [
            {
              "enabled": true,
              "data_stream": {
                "type": "logs",
                "dataset": "microsoft_dnsserver.analytical"
              },
              "vars": {
                "session_name": {
                  "value": "Elastic-DNSServer-Analytical",
                  "type": "text"
                },
                "logfile": {
                  "type": "text"
                },
                "trace_level": {
                  "value": "information",
                  "type": "text"
                },
                "match_all_keyword": {
                  "value": "0x0000000000000000",
                  "type": "text"
                },
                "tags": {
                  "value": [],
                  "type": "text"
                },
                "preserve_duplicate_custom_fields": {
                  "value": false,
                  "type": "bool"
                },
                "processors": {
                  "type": "yaml"
                }
              },
              "compiled_stream": {
                "condition": "${host.platform} == 'windows'",
                "provider.name": "Microsoft-Windows-DNSServer",
                "match_any_keyword": "0x8000000000000000",
                "session_name": "Elastic-DNSServer-Analytical",
                "trace_level": "information",
                "match_all_keyword": "0x0000000000000000",
                "tags": null
              }
            }
          ]
        },
        {
          "type": "winlog",
          "policy_template": "microsoft_dnsserver",
          "enabled": true,
          "streams": [
            {
              "enabled": true,
              "data_stream": {
                "type": "logs",
                "dataset": "microsoft_dnsserver.audit"
              },
              "vars": {
                "preserve_original_event": {
                  "type": "bool",
                  "value": true
                },
                "event_id": {
                  "type": "text"
                },
                "ignore_older": {
                  "value": "72h",
                  "type": "text"
                },
                "language": {
                  "value": 0,
                  "type": "text"
                },
                "preserve_duplicate_custom_fields": {
                  "value": false,
                  "type": "bool"
                },
                "tags": {
                  "value": [],
                  "type": "text"
                },
                "processors": {
                  "type": "yaml"
                }
              },
              "compiled_stream": {
                "name": "Microsoft-Windows-DNSServer/Audit",
                "condition": "${host.platform} == 'windows'",
                "ignore_older": "72h",
                "include_xml": true,
                "tags": [
                  "preserve_original_event"
                ]
              }
            }
          ]
        }
      ]
  }