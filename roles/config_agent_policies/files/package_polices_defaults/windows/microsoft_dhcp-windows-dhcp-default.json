{
  "id": "microsoft_dhcp-windows-dhcp-default",
  "name": "microsoft_dhcp (windows-dhcp-default)",
  "namespace": "",
  "policy_id": "windows-dhcp-default",
  "package": {
    "name": "microsoft_dhcp",
    "version": ""
  },
  "inputs": [
    {
      "type": "logfile",
      "policy_template": "microsoft_dhcp",
      "enabled": true,
      "streams": [
        {
          "enabled": true,
          "data_stream": {
            "type": "logs",
            "dataset": "microsoft_dhcp.log"
          },
          "vars": {
            "tz_offset": {
              "value": "local",
              "type": "text"
            },
            "paths": {
              "value": [
                "C:\\Windows\\System32\\DHCP\\DhcpSrvLog-*.log"
              ],
              "type": "text"
            },
            "preserve_original_event": {
              "type": "bool",
              "value": true
            },
            "tags": {
              "value": [
                "forwarded",
                "microsoft_dhcp"
              ],
              "type": "text"
            },
            "processors": {
              "type": "yaml"
            }
          },
          "compiled_stream": {
            "paths": [
              "C:\\Windows\\System32\\DHCP\\DhcpSrvLog-*.log"
            ],
            "exclude_files": [
              "\\.gz$"
            ],
            "tags": [
              "preserve_original_event",
              "forwarded",
              "microsoft_dhcp"
            ],
            "publisher_pipeline.disable_host": true,
            "fields_under_root": true,
            "fields": {
              "_conf": {
                "tz_offset": "local"
              }
            },
            "processors": [
              {
                "drop_event": {
                  "when": {
                    "not": {
                      "regexp": {
                        "message": "^[0-9]+,.*"
                      }
                    }
                  }
                }
              },
              {
                "add_observer_metadata": null
              },
              {
                "add_locale": null
              }
            ]
          }
        }
      ]
    }
  ]
}