{
  "id": "smbserver-windows-exchange-default",
  "name": "SMBServer (windows-exchange-default)",
  "namespace": "",
  "policy_id": "windows-exchange-default",
  "package": {
    "name": "winlog",
    "version": ""
  },
  "inputs": [
    {
      "type": "winlog",
      "policy_template": "winlogs",
      "enabled": true,
      "streams": [
        {
          "enabled": true,
          "data_stream": {
            "type": "logs",
            "dataset": "winlog.winlogs",
            "elasticsearch": {
              "dynamic_dataset": true,
              "dynamic_namespace": true
            }
          },
          "vars": {
            "channel": {
              "type": "text",
              "value": "Microsoft-Windows-SMBServer/Security"
            },
            "data_stream.dataset": {
              "type": "text",
              "value": "winlog.winlog"
            },
            "pipeline": {
              "type": "text"
            },
            "preserve_original_event": {
              "type": "bool",
              "value": true
            },
            "providers": {
              "value": [],
              "type": "text"
            },
            "event_id": {
              "type": "text"
            },
            "ignore_older": {
              "value": "72h",
              "type": "text"
            },
            "language": {
              "value": 0,
              "type": "text"
            },
            "tags": {
              "value": [],
              "type": "text"
            },
            "custom": {
              "value": "# Winlog configuration example\n#processors:\n#  - drop_event.when.not.or:\n#    - equals.winlog.event_id: '903'\n#    - equals.winlog.event_id: '1024'",
              "type": "yaml"
            }
          },
          "compiled_stream": {
            "condition": "${host.platform} == 'windows'",
            "data_stream": {
              "dataset": "winlog.winlog"
            },
            "name": "Microsoft-Windows-SMBServer/Security",
            "ignore_older": "72h",
            "tags": [
              "preserve_original_event"
            ],
            "include_xml": true
          }
        }
      ]
    }
  ]
}