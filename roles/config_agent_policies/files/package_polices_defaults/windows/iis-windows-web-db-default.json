{
    "id": "iis-windows-web-db-default",
    "name": "IIS (windows-web-db-default)",
    "namespace": "",
    "policy_id": "windows-web-db-default",
    "package": {
      "name": "iis",
      "version": ""
    },
    "inputs": [
        {
          "type": "logfile",
          "policy_template": "iis",
          "enabled": true,
          "streams": [
            {
              "enabled": true,
              "data_stream": {
                "type": "logs",
                "dataset": "iis.access"
              },
              "vars": {
                "paths": {
                  "value": [
                    "C:/inetpub/logs/LogFiles/*/*.log"
                  ],
                  "type": "text"
                },
                "tags": {
                  "value": [
                    "iis-access"
                  ],
                  "type": "text"
                },
                "preserve_original_event": {
                  "type": "bool",
                  "value": true
                },
                "processors": {
                  "type": "yaml"
                },
                "ignore_older": {
                  "value": "72h",
                  "type": "text"
                }
              },
              "compiled_stream": {
                "ignore_older": "72h",
                "paths": [
                  "C:/inetpub/logs/LogFiles/*/*.log"
                ],
                "tags": [
                  "preserve_original_event",
                  "iis-access"
                ],
                "exclude_files": [
                  ".gz$"
                ],
                "exclude_lines": [
                  "^#"
                ]
              }
            },
            {
              "enabled": true,
              "data_stream": {
                "type": "logs",
                "dataset": "iis.error"
              },
              "vars": {
                "paths": {
                  "value": [
                    "C:/Windows/System32/LogFiles/HTTPERR/*.log"
                  ],
                  "type": "text"
                },
                "tags": {
                  "value": [
                    "iis-error"
                  ],
                  "type": "text"
                },
                "preserve_original_event": {
                  "type": "bool",
                  "value": true
                },
                "processors": {
                  "type": "yaml"
                },
                "ignore_older": {
                  "value": "72h",
                  "type": "text"
                }
              },
              "compiled_stream": {
                "ignore_older": "72h",
                "paths": [
                  "C:/Windows/System32/LogFiles/HTTPERR/*.log"
                ],
                "tags": [
                  "preserve_original_event",
                  "iis-error"
                ],
                "exclude_files": [
                  ".gz$"
                ],
                "exclude_lines": [
                  "^#"
                ]
              }
            }
          ],
          "vars": {
            "condition": {
              "type": "text"
            }
          }
        },
        {
          "type": "iis/metrics",
          "policy_template": "iis",
          "enabled": false,
          "streams": [
            {
              "enabled": false,
              "data_stream": {
                "type": "metrics",
                "dataset": "iis.application_pool"
              },
              "vars": {
                "name": {
                  "value": [],
                  "type": "text"
                },
                "processors": {
                  "type": "yaml"
                }
              }
            },
            {
              "enabled": false,
              "data_stream": {
                "type": "metrics",
                "dataset": "iis.webserver"
              },
              "vars": {
                "processors": {
                  "type": "yaml"
                }
              }
            },
            {
              "enabled": false,
              "data_stream": {
                "type": "metrics",
                "dataset": "iis.website"
              },
              "vars": {
                "processors": {
                  "type": "yaml"
                }
              }
            }
          ],
          "vars": {
            "period": {
              "value": "10s",
              "type": "text"
            },
            "condition": {
              "type": "text"
            }
          }
        }
      ]
  }