    },
    {
      "type": "apache/metrics",
      "policy_template": "apache",
      "enabled": false,
      "streams": [
        {
          "enabled": false,
          "data_stream": {
            "type": "metrics",
            "dataset": "apache.status"
          },
          "vars": {
            "period": {
              "value": "30s",
              "type": "text"
            },
            "server_status_path": {
              "value": "/server-status",
              "type": "text"
            }
          }
        }
      ],
      "vars": {
        "hosts": {
          "value": [
            "http://127.0.0.1"
          ],
          "type": "text"
        },
        "condition": {
          "type": "text"
        }
      }
    }
  ]
}