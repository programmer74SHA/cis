{
  "priority": 10,
  "template": {
    "settings": {
      "index": {
        "lifecycle": {
          "name": ".alerts-ilm-policy",
          "rollover_alias": ".alerts-security.alerts-default"
        },
        "mapping": {
          "total_fields": {
            "limit": "2500"
          },
          "ignore_malformed": "true"
        },
        "hidden": "true",
        "auto_expand_replicas": "0-1"
      }
    },
    "mappings": {
      "_meta": {
        "managed": true,
        "namespace": "default",
        "kibana": {
          "version": "8.18.2"
        }
      },
      "dynamic": false,
      "properties": {
        "data_stream": {
          "type": "object",
          "properties": {
            "namespace": {
              "type": "keyword",
              "fields": {
                "text": {
                  "type": "text"
                }
              }
            }
          }
        },
        "sensor": {
          "type": "object",
          "properties": {
            "ip": {
              "type": "ip"
            }
          }
        },
        "data_source": {
          "type": "object",
          "properties": {
            "name": {
              "type": "keyword"
            }
          }
        }
      }	  
    },
    "aliases": {
      ".siem-signals-default": {
        "is_write_index": false
      }
    }
  },
  "index_patterns": [
    ".internal.alerts-security.alerts-default-*"
  ],
  "composed_of": [
    ".alerts-ecs-mappings",
    ".alerts-security.alerts-mappings",
    ".alerts-legacy-alert-mappings",
    ".alerts-framework-mappings"
  ],
  "_meta": {
    "managed": true,
    "namespace": "default",
    "kibana": {
      "version": "8.18.2"
    }
  }
}